<h1 align="center">Food Menu</h1>
{{# each items }}
  <div class="row">
    {{# each this}}
      <div class="col-sm-6 col-md-4">
        <div class="thumbnail">
          <img class="img-responsive" src="{{ this.imagePath }}" >
          <div class="caption">
            <h3>{{ this.item }}</h3>
            <div class="clearfix">
              <div class="price pull-left">Rs {{ this.price }}</div>
              <a href="/add-to-cart/{{ this._id }}" class="btn btn-success pull-right" role="button">Add to Cart</a>
            </div>
          </div>
        </div>
      </div>
    {{/each}}
  </div>
{{/each}}
